          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/resources/tsconfig.lib.json",
              "projects/resources/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "session": {
      "projectType": "library",
      "root": "projects/session",
      "sourceRoot": "projects/session/src",
      "prefix": "lib",
      "architect": {
        "build": {
